{
  "name": "bug-service",
  "version": "1.0.0",
  "description": "Bug management service for Bug Tracker SaaS",
  "main": "dist/server.js",
  "prisma": {
    "schema": "./prisma/schema.prisma"
  },
  "scripts": {
    "postinstall": "prisma generate --schema=./prisma/schema.prisma",
