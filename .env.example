# Frontend Environment Variables (Next.js)

# API URLs
NEXT_PUBLIC_API_URL=http://localhost:5001
NEXT_PUBLIC_BUG_SERVICE_URL=http://localhost:5002
NEXT_PUBLIC_NOTIFICATION_SERVICE_URL=http://localhost:5003
NEXT_PUBLIC_SOCKET_URL=http://localhost:5003

# OAuth Providers (Get credentials from respective platforms)
# GitHub: https://github.com/settings/developers
# Google: https://console.cloud.google.com/
NEXT_PUBLIC_GITHUB_CLIENT_ID=your-github-client-id
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your-google-client-id

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development
